#include <iostream>
using namespace std;
int count(int arr[])
{
    int count=0;
    for (int i=0; i<4; i++)
    {
        if (arr[i]>=10)
        count ++;
    }
    return count;
}
int main() 
{
    int arr[4];
    for (int i=0; i<4; i++)
    {
        cin>>arr[i];
    }
    int x=count(arr);
    cout<<x;
	return 0;
}
