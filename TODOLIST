#include <iostream>
using namespace std;
#include <vector>
int main() 
{
    int t;
    cin>>t;
    while (t--)
    {
        int n,count=0;
        cin>>n;
        vector<int>arr(n);
        for (int i=0; i<n; i++)
        {
            cin>>arr[i];
        }
        for (int j=0; j<n; j++)
        {
            if (arr[j]>=1000)
            {
                count++;
            }
            
        }
        cout<<count<<endl;
        
    }
	return 0;
}
